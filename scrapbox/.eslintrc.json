{
    "extends": "@hakatashi",
    "parser": "@typescript-eslint/parser",
    "env": {
        "jest/globals": true
    },
    "plugins": [
        "jest"
    ],
    "rules": {
        "no-console": "off",
        "camelcase": "off",
        "valid-jsdoc": [
            "error", {
                "requireParamType": false,
                "requireReturnType": false
            }
        ]
    }
}
