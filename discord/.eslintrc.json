{
    "extends": "@hakatashi/eslint-config/typescript",
    "parser": "@typescript-eslint/parser",
    "rules": {
        "no-console": "off",
        "camelcase": "off",
        "no-restricted-imports": "off",
        "import/extesions": "off",
        "require-atomic-updates": "off",
        "no-unused-vars": "off",
        "no-shadow": "off",
        "no-promise-executor-return": "off",
        "no-undef": "off",
        "no-negated-condition": "off"
    }
}
