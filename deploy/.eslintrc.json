{
    "extends": "@hakatashi",
    "parser": "@typescript-eslint/parser",
    "rules": {
        "no-console": "off",
        "camelcase": "off",
        "no-promise-executor-return": "off",
        "no-restricted-imports": "off",
        "import/extensions": "off",
        "no-async-promise-executor": "off",
        "no-return-assign": "off",
        "callback-return": "off",
        "node/callback-return": "off"
    }
}
