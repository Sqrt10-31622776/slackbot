{
    "extends": "@hakatashi",
    "parserOptions": {
        "ecmaVersion": 2017,
        "ecmaFeatures": {
            "experimentalObjectRestSpread": true,
            "jsx": true
        },
        "sourceType": "module"
    },
    "rules": {
        "no-console": "off",
        "no-div-regex": "off",
        "no-nested-ternary": "off",
        "no-use-before-define": "off"
    }
}
